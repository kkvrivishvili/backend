# Variables de entorno para desarrollo local
# Versión de prueba - configurada para tests locales

# API Keys
OPENAI_API_KEY=sk-mock-testing-key-for-local-deployment

# Supabase
SUPABASE_URL=mock
SUPABASE_KEY=mock_key_for_testing

# Redis
REDIS_URL=redis://localhost:6379/0

# Configuración de servicios
EMBEDDING_SERVICE_URL=http://localhost:8001
QUERY_SERVICE_URL=http://localhost:8002
INGESTION_SERVICE_URL=http://localhost:8000
AGENT_SERVICE_URL=http://localhost:8003

# Modo de prueba (sin Supabase)
SKIP_SUPABASE=true
TESTING_MODE=true

# Nivel de logs
LOG_LEVEL=DEBUG

# Puerto para los servicios (cuando se ejecutan fuera de Docker)
EMBEDDING_SERVICE_PORT=8001
QUERY_SERVICE_PORT=8002
INGESTION_SERVICE_PORT=8000
AGENT_SERVICE_PORT=8003