# backend/server-llama/.env.example
# Archivo de configuración de entorno para los servicios Linktree AI

# Supabase Configuration
SUPABASE_URL=https://your-supabase-project.supabase.co
SUPABASE_KEY=your-supabase-service-key

# OpenAI API Configuration
OPENAI_API_KEY=your-openai-api-key

# Redis Configuration
REDIS_URL=redis://redis:6379/0

# Service Settings
SERVICE_VERSION=1.0.0
SERVICE_NAME=linktree-ai

# Service URLs (para comunicación interna)
EMBEDDING_SERVICE_URL=http://embeddings-service:8001
INGESTION_SERVICE_URL=http://ingestion-service:8000
QUERY_SERVICE_URL=http://query-service:8002
AGENT_SERVICE_URL=http://agent-service:8003

# Embedding Settings
DEFAULT_EMBEDDING_MODEL=text-embedding-3-small
DEFAULT_EMBEDDING_DIMENSION=1536
EMBEDDING_BATCH_SIZE=100

# LLM Settings
DEFAULT_LLM_MODEL=gpt-3.5-turbo

# Rate Limits
RATE_LIMIT_FREE_TIER=600
RATE_LIMIT_PRO_TIER=1200
RATE_LIMIT_BUSINESS_TIER=3000

# Cache
CACHE_TTL=86400

# Logging
LOG_LEVEL=INFO